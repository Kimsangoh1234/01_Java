package ncs.test5;

public class BookArrayTest {
public static void main(String[] args) {
	Maptest[]bArray = new Maptest[3];
	
	bArray[0]= new Maptest("자바의 정석", "남궁성", 30000, "도우출판",0.1);
	bArray[1]= new Maptest("열혈강의 자바", "구정은", 30000, "프리렉",0.1);
	bArray[2]= new Maptest("객체지향 Java8", "금영욱", 30000, "북스홈",0.1);

for(Maptest b: bArray	) {
	System.out.println("Title : "+b.getTitle());
	System.out.println("author : "+b.getAuthor());
	System.out.println("price : "+b.getPrice());
	System.out.println("publisher : "+b.getPublisher());
	System.out.println("discountrate : "+b.getDiscountrate());
}
System.out.println(bArray);
}
}